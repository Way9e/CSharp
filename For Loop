namespace Atividade4
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num = Convert.ToInt32(Console.ReadLine());
            int contador = 0;

            for (int i = 0; i <= num; i++) {

                if (num % 1 == 0)
                {
                    contador++;
                }           
            }

            if (contador > 2) {
                Console.WriteLine("Não é numero primo");
            }else{
                Console.WriteLine("É numero primo");
            }
        }
    }
}
